{
  "name": "@jakoblierman/eslint-config",
  "version": "0.0.0",
  "description": "Personal ESLint configuration",
  "author": {
    "name": "Jakob Lierman",
    "email": "jakob.lierman@gmail.com",
    "url": "https://jakoblierman.be"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakobLierman/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/JakobLierman/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "engines": {
    "node": ">=v20"
  },
  "files": [
    "index.js",
    "react.js",
    "nextjs.js",
    "jest.js",
    "vitest.js"
  ],
  "main": "index.js",
  "scripts": {
    "format": "prettier .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "release": "release-it",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^2.2.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-xss": "^0.1.12"
  },
  "peerDependencies": {
    "eslint": "^8.56.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@jakoblierman/commitlint-config": "^1.0.0",
    "@jakoblierman/prettier-config": "^1.0.0",
    "@jest/globals": "^29.7.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/eslint": "^8.56.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "latest",
    "prettier": "^3.2.4",
    "release-it": "^17.0.1"
  },
  "license": "MIT"
}
